{
    "schemaVersion": "0.1.0",
    "meta": {
        "version": "1.0.0",
        "name": "FotosHochladen"
    },
    "uploader": {
        "fileFormName": "upload",
        "requestUrl": "http://www.fotos-hochladen.net/",
        "postParams": {
            "resize": "no-resize",
            "bedingungen":"akzeptiert",
            "submit":"Bild hochladen",
        },
        "parser": {
            "kind": "regex",
            "success": "(?<url>http://[a-zA-Z0-9]{1,}\\.fotos-hochladen\\.net/uploads/(.+)\\.[a-zA-Z]{1,3})",
            "url": "$3,url$"
        }
    }
}
