{
    "schemaVersion": "0.1.0",
    "info": {
        "version": "1.0.0",
        "name": "DirectUpload.net"
    },
    "uploader": {
        "fileFormName": "bilddatei",
        "requestUrl": "https://directupload.net",
        "postParams": {
            "input": "file"
        },
        "parser": {
            "kind": "regex",
            "success": "(?<url>http://[a-zA-Z0-9]*.directupload.net/images/\\d{1,}/\\w{1,}.[a-zA-Z]{1,3})",
            "url": "$3,url$"
        }
    }
}
